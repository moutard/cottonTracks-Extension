{
  "name": "CottonTracks",
  "version": "1.0",
  "description": "Reads your history, and shows the top ten pages you go to by typing the URL.",
  "permissions": ["history", "tabs", "http://*/*", "https://*/*"],
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "css": [],
    "js": ["lib/class.js", "lib/jquery.min.js", "lib/raphael.min.js", "lib/underscore.min.js", "lib/backbone.min.js", "lib/date.format.js", "lib/parseURL.js", "lib/underscore.js", "init.js", "behavior/init.js", "behavior/passive/init.js", "behavior/passive/parser.js", "behavior/active/init.js", "behavior/active/highlighter.js"]
    }]
}
