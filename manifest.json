{
  "name": "CottonTracks",
  "version": "1.0",
  "description": "Reads your history, and shows the top ten pages you go to by typing the URL.",
  "permissions": ["history", "tabs", "http://*/*", "https://*/*"],
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "css": [],
      "js": ["lib/class.js", "lib/jquery.min.js", "lib/jquery.livequery.js", "lib/underscore.min.js", "init.js", "behavior/init.js", "behavior/passive/init.js", "behavior/passive/parser.js", "behavior/active/init.js", "behavior/active/reading_rater.js", "behavior/active/highlighter.js"],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self' https://ssl.google-analytics.com; object-src 'self'"
}
